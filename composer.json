{
    "name": "inspiredminds/contao-austrian-administrative-areas",
    "description": "Contao extension to provide a form field for Austrian administrative areas (e.g. districts and municipalities).",
    "keywords": ["contao", "austria", "administrative", "district", "form", "select", "postal", "zip"],
    "type": "contao-bundle",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Fritz Michael Gschwantner",
            "homepage": "http://www.inspiredminds.at",
            "email": "fmg@inspiredminds.at",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/inspiredminds/contao-austrian-administrative-areas/issues",
        "source": "https://github.com/inspiredminds/contao-austrian-administrative-areas",
        "forum": "https://community.contao.org/de"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/fritzmg"
        }
    ],
    "require": {
        "php": ">=8.1",
        "contao/core-bundle": "^4.13 || ^5.0",
        "symfony/config": "^5.4 || ^6.4",
        "symfony/http-kernel": "^5.4 || ^6.4",
        "symfony/cache": "^5.4 || ^6.4",
        "league/csv": "^9.15"
    },
    "require-dev": {
        "contao/easy-coding-standard": "^6.0",
        "contao/rector": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "InspiredMinds\\ContaoAustrianAdministrativeAreas\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "InspiredMinds\\ContaoAustrianAdministrativeAreas\\ContaoManager\\Plugin"
    },
    "config": {
        "allow-plugins": {
            "contao-components/installer": true,
            "php-http/discovery": false,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
